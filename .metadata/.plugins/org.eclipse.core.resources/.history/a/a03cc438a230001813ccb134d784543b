
public class HW_2_25 {

	public static void main(String[] args) {
/*
다음과 같은 출력이 나오도록 프로그램을 작성하시오.
(1) *********** (2)54321012345
********* 432101234
******* 3210123
***** 21012
*** 101
* 0
*** 101 
***** 21012 
******* 3210123 
********* 432101234 
*********** 54321012345
*/
		int c=11;
		int d=0;
		for(int a=0;a<11;a++) {
			//내부 함수 실행
			for(int b=1;b<=c;b++) {
				System.out.print("*");
			}
			if(c==1) {
				d=1;
			}
			if(d==0) {
				c=c-2;
			}
			if(d==1) {
				c=c+2;
			}
			System.out.println();
		}
		
		
		int e=11;
		int f=0;
		int h=-1;
		for(int a=0;a<11;a++) {
			//내부 함수 실행
			for(int b=1;b<=c;b++) {
				for(int g=5;g<=5;g=g+h) {
			//		System.out.println()
				}
			}
			if(f==1) {
				e=1;
			}
			if(f==0) {
				e=e-2;
			}
			if(f==1) {
				e=e+2;
			}
			System.out.println();
		}
		
		
		System.out.println("=====프로그램 종료=====");
	}
}
			
	


